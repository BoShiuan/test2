		public void Main()
		{
            string file_folder_date = Dts.Variables["final_file_folder_date"].Value.ToString();
            string final_file_name = Dts.Variables["final_file_name"].Value.ToString();
            string final_file_name_xlsx = Dts.Variables["final_file_name_xlsx"].Value.ToString();


            if (!System.IO.Directory.Exists(file_folder_date))
                System.IO.Directory.CreateDirectory(file_folder_date);

            var final_file_folder_date = new DirectoryInfo(Dts.Variables["final_file_folder_date"].Value.ToString());
            FileInfo[] csvfiles = final_file_folder_date.GetFiles();
            foreach (FileInfo file in csvfiles)
            {
